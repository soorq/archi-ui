{
  "version": "0.1.0",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test --passWithNoTests",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "fancy-ui",
  "author": "fancy-ui-react-lib",
  "module": "dist/fancy-ui.esm.js",
  "size-limit": [
    {
      "path": "dist/fancy-ui.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/fancy-ui.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@babel/core": "^7.24.6",
    "@size-limit/preset-small-lib": "^11.1.4",
    "@storybook/addon-essentials": "^8.1.4",
    "@storybook/addon-info": "^5.3.21",
    "@storybook/addon-links": "^8.1.4",
    "@storybook/addons": "^7.6.17",
    "@storybook/react": "^8.1.4",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "autoprefixer": "^10.4.19",
    "babel-loader": "^9.1.3",
    "cssnano": "^7.0.1",
    "husky": "^9.0.11",
    "postcss": "^8.4.38",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-is": "^18.3.1",
    "rollup-plugin-postcss": "^4.0.2",
    "size-limit": "^11.1.4",
    "tsdx": "^0.14.1",
    "tslib": "^2.6.2",
    "typescript": "^5.4.5"
  }
}
